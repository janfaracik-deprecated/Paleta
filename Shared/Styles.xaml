                    <Grid x:Name="RootGrid" Background="{TemplateBinding Background}" CornerRadius="20" Width="40" Height="40" VerticalAlignment="Center" HorizontalAlignment="Center">

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
